package november_30th_2017;

class ImplementRunnable implements Runnable {
	int number;

	ImplementRunnable(int no) {
		number = no;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		try {
			Thread.sleep(1000);
		}
	}

}

public class ImplementRunnableInterface {
	public static void main(String[] args) {

	}
}
